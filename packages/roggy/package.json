{
  "name": "roggy",
  "version": "0.1.1",
  "description": "Fetch any markdown documentation as raw markdown strings",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage",
    "prepublishOnly": "yarn build"
  },
  "repository": "https://github.com/HashimotoYT/roggy",
  "keywords": [
    "api",
    "documentation",
    "markdown",
    "reference",
    "schema",
    "raw"
  ],
  "dependencies": {
    "got": "^11.7.0",
    "gunzip-maybe": "^1.4.2",
    "node-dir": "^0.1.17",
    "tar-fs": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^14.11.2",
    "@types/node-dir": "^0.0.33",
    "@types/tar-fs": "^2.0.0"
  },
  "engines": {
    "node": ">= 10"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  }
}
